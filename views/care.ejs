<style>
    *{
       background-color: rgb(117, 193, 243);
       font-family: 'roboto',sans-serif;
       font-weight: 900;
       font-size: large;
    }
   
    
</style>

<table class="table table-dark">
    <tbody>
      <tr>
        <th scope="col">Sr. No.</th>
        <th scope="col">name</th>
        <th scope="col">complaint</th>
      </tr>
      <% obj.forEach(function(entry, i, entries){ %>
      <tr>
        <th scope="row">
          <div><%= i+1 %></div>
        </th>
        <td colspan="2" class="table-active"><div><%= entry.name %></div></td>
        <td><div><%= entry.complaint %></div></td>
      </tr>
      <% } )%>
    </tbody>
  </table>